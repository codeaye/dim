{
  "build": {
    "devPath": "http://localhost:5173",
    "beforeDevCommand": "yarn dev",
    "distDir": "../dist",
    "beforeBuildCommand": "yarn build"
  },
  "package": {
    "productName": "Dim",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "window": {
        "all": false,
        "startDragging": true
      }
    },
    "bundle": {
      "active": true,
      "category": "Productivity",
      "copyright": "@codeaye",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.nav.dev",
      "shortDescription": "A handy little password generator",
      "longDescription": "A handy little password generator",
      "resources": [],
      "targets": "all",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "title": "Dim",
        "width": 380,
        "height": 310,
        "fullscreen": false,
        "resizable": false
      }
    ]
  }
}
